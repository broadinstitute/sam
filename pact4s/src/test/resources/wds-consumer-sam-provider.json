{
  "provider": {
    "name": "sam-provider"
  },
  "consumer": {
    "name": "wds-consumer"
  },
  "interactions": [
    {
      "description": "a status request",
      "request": {
        "method": "GET",
        "path": "/status"
      },
      "response": {
        "status": 500,
        "body": {
          "ok": false
        }
      },
      "providerStates": [
        {
          "name": "Sam is not ok"
        }
      ]
    },
    {
      "description": "a status request",
      "request": {
        "method": "GET",
        "path": "/status"
      },
      "response": {
        "status": 200,
        "body": {
          "ok": true
        }
      },
      "providerStates": [
        {
          "name": "Sam is ok"
        }
      ]
    },
    {
      "description": "a request for the user's status",
      "request": {
        "method": "GET",
        "path": "/register/user/v2/self/info"
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json; charset=UTF-8"
        },
        "body": {
          "userSubjectId": "string",
          "userEmail": "string",
          "enabled": true
        },
        "matchingRules": {
          "body": {
            "$.userSubjectId": {
              "matchers": [
                {
                  "match": "type"
                }
              ],
              "combine": "AND"
            },
            "$.userEmail": {
              "matchers": [
                {
                  "match": "type"
                }
              ],
              "combine": "AND"
            },
            "$.enabled": {
              "matchers": [
                {
                  "match": "type"
                }
              ],
              "combine": "AND"
            }
          },
          "header": {
            "Content-Type": {
              "matchers": [
                {
                  "match": "regex",
                  "regex": "application/json(;\\s?charset=[\\w\\-]+)?"
                }
              ],
              "combine": "AND"
            }
          }
        },
        "generators": {
          "body": {
            "$.userSubjectId": {
              "type": "RandomString",
              "size": 20
            },
            "$.userEmail": {
              "type": "RandomString",
              "size": 20
            }
          }
        }
      }
    },
    {
      "description": "a request for the user's status without a token",
      "request": {
        "method": "GET",
        "path": "/register/user/v2/self/info"
      },
      "response": {
        "status": 401
      }
    },
